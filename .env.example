# 数据库配置示例
# 复制此文件为.env并填写实际配置

# 数据库连接信息
DB_HOST=localhost
DB_USER=username
DB_PASSWORD=your_secure_password
DB_NAME=your_database
DB_PORT=3306
DB_CHARSET=utf8

# API配置
API_BASE_URL=http://api.example.com
API_TIMEOUT=30
API_RETRY_COUNT=3
API_RETRY_DELAY=1.0

# 日志配置
LOG_LEVEL=INFO
LOG_FILE_LEVEL=DEBUG
LOG_CONSOLE_LEVEL=INFO

# 测试配置
DEFAULT_PROJECT=your_project
MAX_WORKERS=4